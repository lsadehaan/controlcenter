{
  "agentId": "0d99440f-93a0-4ba9-95ab-7764c81bad14",
  "managerUrl": "http://localhost:3000",
  "registered": true,
  "sshPrivateKeyPath": "C:\\Users\\ade_h\\.controlcenter-agent\\agent_key",
  "sshPublicKeyPath": "C:\\Users\\ade_h\\.controlcenter-agent\\agent_key.pub",
  "configRepoPath": "C:\\Users\\ade_h\\.controlcenter-agent\\config-repo",
  "stateFilePath": "C:\\Users\\ade_h\\.controlcenter-agent\\state.json",
  "logFilePath": "C:\\Users\\ade_h\\.controlcenter-agent\\agent.log",
  "sshServerPort": 2222,
  "authorizedSshKeys": null,
  "workflows": [
    {
      "id": "example-file-processor",
      "name": "File Processing Workflow",
      "description": "Process CSV files when they appear in the watch directory",
      "enabled": false,
      "trigger": {
        "type": "file",
        "config": {
          "path": "C:\\temp\\watch",
          "pattern": "*.csv",
          "recursive": false
        }
      },
      "steps": [
        {
          "id": "step-1",
          "type": "alert",
          "name": "",
          "config": {
            "level": "info",
            "message": "New file detected: {{.Trigger.Filename}}"
          },
          "next": [
            "step-2"
          ]
        },
        {
          "id": "step-2",
          "type": "copy-file",
          "name": "",
          "config": {
            "destination": "C:\\temp\\backup\\{{.Trigger.Filename}}",
            "source": "{{.Trigger.FilePath}}"
          },
          "next": [
            "step-3"
          ]
        },
        {
          "id": "step-3",
          "type": "move-file",
          "name": "",
          "config": {
            "destination": "C:\\temp\\processed\\{{.Trigger.Filename}}",
            "source": "{{.Trigger.FilePath}}"
          },
          "next": [
            "step-4"
          ]
        },
        {
          "id": "step-4",
          "type": "alert",
          "name": "",
          "config": {
            "level": "info",
            "message": "File processed successfully: {{.Trigger.Filename}}"
          }
        }
      ]
    },
    {
      "id": "example-scheduled-task",
      "name": "Scheduled Cleanup",
      "description": "Run cleanup every 5 minutes",
      "enabled": false,
      "trigger": {
        "type": "schedule",
        "config": {
          "interval": 300
        }
      },
      "steps": [
        {
          "id": "cleanup-1",
          "type": "command",
          "name": "",
          "config": {
            "args": [
              "-Command",
              "Get-ChildItem C:\\temp\\processed -Filter *.tmp | Remove-Item"
            ],
            "command": "powershell",
            "working_directory": "C:\\temp"
          },
          "next": [
            "cleanup-2"
          ]
        },
        {
          "id": "cleanup-2",
          "type": "alert",
          "name": "",
          "config": {
            "level": "info",
            "message": "Cleanup completed"
          }
        }
      ]
    }
  ]
}