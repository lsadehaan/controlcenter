{
  "id": "test-webhook-workflow",
  "name": "Test Webhook Workflow",
  "description": "Testing webhook trigger",
  "enabled": true,
  "trigger": {
    "type": "webhook",
    "config": {
      "path": "/api/webhooks/test",
      "method": "POST"
    }
  },
  "steps": [
    {
      "id": "alert-step",
      "type": "alert",
      "name": "Webhook Alert",
      "config": {
        "level": "info",
        "message": "Webhook received: {{.webhookData}}"
      },
      "next": []
    }
  ]
}
